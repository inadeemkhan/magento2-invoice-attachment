<?xml version="1.0" ?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
	<route url="/V1/nadeem-pdffileattachment/invoiceattachment" method="POST">
		<service class="Nadeem\PdfFileAttachment\Api\InvoiceAttachmentRepositoryInterface" method="save"/>
		<resources>
			<resource ref="Nadeem_PdfFileAttachment::InvoiceAttachment_save"/>
		</resources>
	</route>
	<route url="/V1/nadeem-pdffileattachment/invoiceattachment/search" method="GET">
		<service class="Nadeem\PdfFileAttachment\Api\InvoiceAttachmentRepositoryInterface" method="getList"/>
		<resources>
			<resource ref="Nadeem_PdfFileAttachment::InvoiceAttachment_view"/>
		</resources>
	</route>
	<route url="/V1/nadeem-pdffileattachment/invoiceattachment/:invoiceattachmentId" method="GET">
		<service class="Nadeem\PdfFileAttachment\Api\InvoiceAttachmentRepositoryInterface" method="get"/>
		<resources>
			<resource ref="Nadeem_PdfFileAttachment::InvoiceAttachment_view"/>
		</resources>
	</route>
	<route url="/V1/nadeem-pdffileattachment/invoiceattachment/:invoiceattachmentId" method="PUT">
		<service class="Nadeem\PdfFileAttachment\Api\InvoiceAttachmentRepositoryInterface" method="save"/>
		<resources>
			<resource ref="Nadeem_PdfFileAttachment::InvoiceAttachment_update"/>
		</resources>
	</route>
	<route url="/V1/nadeem-pdffileattachment/invoiceattachment/:invoiceattachmentId" method="DELETE">
		<service class="Nadeem\PdfFileAttachment\Api\InvoiceAttachmentRepositoryInterface" method="deleteById"/>
		<resources>
			<resource ref="Nadeem_PdfFileAttachment::InvoiceAttachment_delete"/>
		</resources>
	</route>
</routes>
